$temp_de=0;
$temp_ca=0;
$temp_gr=0;
for($i=0;$i<count($tab);$i++){
$a=explode(".",($tab[$i]));
$b=array($a[3]);
if($b[0]=="pl"){
$temp_pl++;
//$w=count($b);{
//echo "liczba pl: " .$temp_pl."\n";
}
if($b[0]=="de"){
$temp_de++;
//echo "liczba de: " .$temp_de."\n";
}
if($b[0]=="gr"){
$temp_gr++;
//echo "liczba de: " .$temp_de."\n";
}
