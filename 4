$y4=234;
$x5=43;
$y5=34;

$x5=6;
$y5=234;
$x6=43;
$y6=76;
$x6=3;
$y6=324;
$x7=43;
$y7=58;
$x7=2;
$y7=65;
$x8=43;
$y8=23;
$x8=3;
$y8=543;
$x9=43;
$y9=153;
$x9=7;
$y9=3;
$x10=43;
$y10=534;
$x10=2;
$y10=23;
$x11=43;
$y11=34;
function eukl($x1,$y1,$x2,$y2){
return sqrt(($x1-$y1)*($x1-$y1)+($x2-$y2)*($x2-$y2));
}
echo eukl($x1,$y1,$x2,$y2)."\n";
echo eukl($x2,$y2,$x3,$y3)."\n";
echo eukl($x3,$y3,$x4,$y4)."\n";
echo eukl($x4,$y4,$x5,$y5)."\n";
echo eukl($x5,$y5,$x6,$y6)."\n";
echo eukl($x6,$y6,$x7,$y7)."\n";
echo eukl($x7,$y7,$x8,$y8)."\n";
echo eukl($x8,$y8,$x9,$y9)."\n";
echo eukl($x9,$y9,$x10,$y10)."\n";
echo eukl($x10,$y10,$x11,$y11)."\n";
?>

Polecenie 9

<?php
$pes[0]="76080615953";
$pes[1]="67072315347";
$pes[2]="82082334952";
$pes[3]="89062334955";
$pes[4]="56051254954";
$pes[5]="65030545953";
$pes[6]="87010365952";
$pes[7]="88020623953";
function wiek($a){
$b=substr($a, 0, 6);
$c=substr($b, 0, 2);
$d=substr($b, 2, 2);
$e=substr($b, 4, 6);
$f="19";
$g=$f.$c;
$wiek=(2015-$g);
if($d<2){
echo "Data urodzenia: " .$e."-".$d."-".$g."\n";
echo "Wiek: " .$wiek;
}
if($d==2 && $e<12 ){
echo "Data urodzenia: " .$e."-".$d."-".$g."\n";
echo "Wiek: " .($wiek);
}
if($d<2 && $e>12){
echo "Data urodzenia: " .$e."-".$d."-".$g."\n";
echo "Wiek: " .($wiek-1);
}
if($d>2){
echo "Data urodzenia: " .$e."-".$d."-".$g."\n";
echo "Wiek: " .($wiek-1);
}
}
for($i=0;$i<count($pes);$i++){
echo wiek($pes[$i])."\n";
}
?>
Polecenie 10
<?php
$a=85;
$b=1.87;
$c=90;

$d=1.63;
function bmi($x,$y){
$bmi=$x/($y*$y);
if($bmi<16){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: < 16,0 – wygłodzenie";
}
if($bmi>=16 && $bmi<18){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: 16,0–16,99 – wychudzenie (spowodowane
często przez ciężką chorobę lub anoreksję)";
}
if($bmi>=17 && $bmi<18.5){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: 17,0–18,49 – niedowaga";
}
if($bmi>=18.5 && $bmi<25){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: 18,5–24,99 – wartość prawidłowa";
}
if($bmi>=25 && $bmi<30){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: 25,0–29,99 – nadwaga";
}
if($bmi>=30 && $bmi<35){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: 30,0–34,99 – I stopień otyłości";
}
if($bmi>=35 && $bmi<40){
echo " Masz $x wagi i $y m wzrostu. Twoje BMI: 35,0–39,99 – II stopień otyłości (otyłość
kliniczna)";
}
if($bmi>40){
echo "Masz $x wagi i $y m wzrostu. Twoje BMI: ≥ 40,0 – III stopień otyłości (otyłość
skrajna)";
}
}
echo bmi($a,$b)."\n";
echo bmi($c,$d);
?>
