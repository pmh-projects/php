Polecenie 6

<?php
$tab[0]="Www.x7x2x6.wp.pl";
$tab[1]="Www.x3x47x6.interia.pl";
$tab[2]="Www.x7x285x6.logo.gr";
$tab[3]="Www.x32781x3x6.vv.de";
$tab[4]="Www.x2x79x6.zip.gr";
$tab[5]="Www.x3x593x6.martis.ca";
$tab[6]="Www.x227x4x6.onet.pl";
$tab[7]="Www.x5x3x6.wp.gr";
$tab[8]="Www.xx1vdfx6.yahoo.de";
$tab[9]="Www.x7x2x6.wp.pl";
$tab[10]="Www.x3x47x6.interia.gr";
$tab[11]="Www.x7x285x6.logo.de";
$tab[12]="Www.x32781x3x6.vv.de";
$tab[13]="Www.x2x79x6.zip.ca";
$tab[14]="Www.x3x593x6.martis.ca";
$tab[15]="Www.x227x4x6.onet.pl";
$tab[16]="Www.x5x3x6.wp.pl";
$tab[17]="Www.xx1vdfx6.yahoo.de";
function licz($x,$y){
$_pl=0;
$_gr=0;
$_ca=0;

$_de=0;
for($i=0;$i<count($x);$i++){
$a=substr($x[$i], -2);
$c=str_split("$a",2);
if($c[0]=="pl"){
$d=count($c);
$_pl++;
}
if($c[0]=="gr"){
$d=count($c);
$_gr++;
}
if($c[0]=="ca"){
$d=count($c);
$_ca++;
}
if($c[0]=="de"){
$d=count($c);
$_de++;
}
}
switch ($y){
case "pl":
echo $_pl;
break;
case "gr":
echo $_gr;
break;
case "ca":
echo $_ca;
break;
case "de":
echo $_de;
break;
}
}
//licz pl
echo licz($tab,"pl")."\n";
//licz gr
echo licz($tab,"gr")."\n";
//licz ca
echo licz($tab,"ca")."\n";
//licz de
echo licz($tab,"de")."\n";
?>

