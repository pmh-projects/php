$tab[3]="dfgnjjkjmhngbdfvgnhbgfdfsvfvsdfvdsbfdvdbhngdfsbdfb";
$tab[4]="mdfbnfxbxbfgbgfbxcbfgdxbdfbgfbfgbffxbjdvfdvsdsvvsdfv";
$c=0;
do{
for($a=0;$a<strlen($tab[$c]);$a++){
if($a%2==0){
$tab[$c][$a]=ucwords($tab[$c][$a]);
}
}
$c++;
}while($c<count($tab));
for($i=0;$i<count($tab);$i++){
echo $tab[$i]."\n";
}
?>

